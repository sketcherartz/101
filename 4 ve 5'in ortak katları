package Sinif;
import java.util.Scanner;
public class varan20 {
    public static void main(String[] args) {
        int i;
        Scanner input = new Scanner(System.in);
        System.out.print("Bir SayÄ± Giriniz:");
        i = input.nextInt();

        int j = 1;
        while(j<=i){
            if (j%4==0&&j%5==0){
                System.out.print(j+",");
            }
            j++;
        }

    }
}
